    "test:robustness": "node scripts/test-robustness.js",
    "test:stress": "node scripts/stress-test.js",
    "test:all": "npm run test:robustness && npm run test:stress"
  },
  "dependencies": {
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.3",
    "firebase-admin": "^13.6.0",
    "node-fetch": "^2.7.0",
    "slugify": "^1.6.6",
    "xlsx": "^0.18.5",
    "zod": "^4.2.1"
  }
}
